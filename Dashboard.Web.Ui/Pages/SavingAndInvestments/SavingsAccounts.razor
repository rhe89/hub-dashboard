@using Dashboard.ViewModels.Sbanken
@using Dashboard.Web.Ui.Helpers

@if (AccountsViewModel?.Accounts == null)
{
    return;
}

@foreach (var account in AccountsViewModel.Accounts)
    {
        <Grid Item>
            <SavingsAccount Account="@account"/>
        </Grid>
    }

@code
{
    [Parameter]
    public AccountsViewModel AccountsViewModel { get; set; }
}